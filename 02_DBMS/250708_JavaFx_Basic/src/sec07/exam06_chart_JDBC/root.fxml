                <CategoryAxis fx:id="xAxis" label="Manager ID"/>
            </xAxis>
            <yAxis>
                <NumberAxis fx:id="yAxis" label="Average Salary"/>
            </yAxis>
        </BarChart>
    </children>
</AnchorPane>
